    "from gpiozero import PWMLED\n",
    "from time import sleep\n",
    "\n",
    "led = PWMLED(17)\n",
    "\n",
    "while True:\n",
    "    led.value = 0  # off\n",
    "    sleep(1)\n",
    "    led.value = 0.5  # half brightness\n",
    "    sleep(1)\n",
    "    led.value = 1  # full brightness\n",
    "    sleep(1)\n",
    "\n"
